    <entry>
      <name>carga_ods_operacional</name>
      <description/>
      <type>SQL</type>
      <sql>-- Inserta los tramos que no existen en el ODS OPERACIONES

insert into ODS_OPERACIONES.dbo.O_ODS_OPERACIONES
select
a.operation_date,
a.comercial_date as local_date,
a.operation_flight_number,
a.comercial_flight_number as flight_number,
a.boardpoint,
a.offpoint,
a.std,
a.sta,
a.stdcorr,
a.status,
a.etd,
a.eta,
a.atd,
a.toff,
a.tdwn,
a.ata,
a.depdelay1,
a.depdur1,
a.depdelay2,
a.depdur2,
a.depdelay3,
a.depdur3,
a.gate,
a.orig_actyp,
a.actyp,
a.ac,
a.arrdelay1,
a.arrdur1,
a.arrdelay2,
a.arrdur2,
a.arrdelay3,
a.arrdur3,
a.depgate,
a.arrgate,
null,
null,
null,
null,
null,
null,
null,
null,
null,
null,
null,
null,
null,
a.leg_number
from ODS_TEMPORALES.dbo.ODS_OPE_T_LEG a
left join ODS_OPERACIONES.dbo.O_ODS_OPERACIONES b on
		a.operation_date=b.operation_date
	and a.operation_flight_number=b.operation_flight_number
	and a.boardpoint=b.boardpoint
	and a.offpoint=b.offpoint
where b.operation_date is null;


-- Borra los tramos del ODS que no existen en el Plan Operacional

delete b
from ODS_TEMPORALES.dbo.ODS_OPE_T_LEG a
right join ODS_OPERACIONES.dbo.O_ODS_OPERACIONES b on
		a.operation_date=b.operation_date
	and a.operation_flight_number=b.operation_flight_number
	and a.boardpoint=b.boardpoint
	and a.offpoint=b.offpoint
where a.operation_date is null
and b.operation_date>cast(getdate() as date);



-- Actualiza los valores de los tramos del ODS con los datos Live desde PDC

update a
set 
a.operation_date=b.operation_date,
a.local_date=b.comercial_date,
a.operation_flight_number=b.operation_flight_number,
a.flight_number=b.comercial_flight_number,
a.boardpoint=b.boardpoint,
a.offpoint=b.offpoint,
a.std=b.std,
a.sta=b.sta,
a.stdcorr=b.stdcorr,
a.status=b.status,
a.etd=b.etd,
a.eta=b.eta,
a.atd=b.atd,
a.toff=b.toff,
a.tdwn=b.tdwn,
a.ata=b.ata,
a.depdelay1=b.depdelay1,
a.depdur1=b.depdur1,
a.depdelay2=b.depdelay2,
a.depdur2=b.depdur2,
a.depdelay3=b.depdelay3,
a.depdur3=b.depdur3,
a.gate=b.gate,
a.orig_actyp=b.orig_actyp,
a.actyp=b.actyp,
a.ac=b.ac,
a.arrdelay1=b.arrdelay1,
a.arrdur1=b.arrdur1,
a.arrdelay2=b.arrdelay2,
a.arrdur2=b.arrdur2,
a.arrdelay3=b.arrdelay3,
a.arrdur3=b.arrdur3,
a.depgate=b.depgate,
a.arrgate=b.arrgate
from ODS_TEMPORALES.dbo.ODS_OPE_T_LEG b
left join ODS_OPERACIONES.dbo.O_ODS_OPERACIONES a on
		a.operation_date=b.operation_date
	and a.operation_flight_number=b.operation_flight_number
	and a.boardpoint=b.boardpoint
	and a.offpoint=b.offpoint;



-- Actualiza los valores de combustible descargados (dos meses moviles)

update a
set a.block_off_fuel=b.block_off_fuel,
	a.block_on_fuel=b.block_on_fuel,
	a.burn_fuel=b.consumo_klg,
	a.fuel_uplift=b.carguio_lts,
	a.apu_burn_fuel=b.apu_kg,
	a.source_fuel=(case when b.fuente='ok' then 'FL' else 'FS' end)
from ODS_TEMPORALES.dbo.ODS_OPE_I_COMBUSTIBLE b
left join ODS_OPERACIONES.dbo.O_ODS_OPERACIONES a on
		a.operation_date=b.fecha_utc
	and a.local_date=b.fecha_local
	and a.flight_number=b.vuelo
	and a.boardpoint=b.origen
	and a.offpoint=b.destino
	and a.ac=b.matricula
	and a.leg_number=b.leg

</sql>
