    <entry>
      <name>ods_ope_t_leg</name>
      <description/>
      <type>SQL</type>
      <sql>truncate table ODS_OPE_T_LEG;

insert into ODS_OPE_T_LEG
select
cast(datop as date) operation_date,
cast(dateadd(hour,case when b.diferencia_horaria is null then -3 else b.diferencia_horaria end,cast(replace(std,'.',':') as datetime)) as date) comercial_date,
case when len(fltid)=7 then right(fltid,4)
	 when len(fltid)=8 then right(fltid,5)
	 end  as operation_flight_number,
case when len(fltid)=7 then cast(replace(replace(replace(right(fltid,4),'A',''),'B',''),'Z','') as int)
	 when len(fltid)=8 then cast(replace(replace(replace(right(fltid,5),'A',''),'B',''),'Z','') as int)
	 end as comercial_flight_number,
DEPSTN as boardpoint,
ARRSTN as offpoint,
cast(replace(std,'.',':') as datetime) as std,
cast(replace(sta,'.',':') as datetime) as sta,
stdcorr,
status,
cast(replace(etd,'.',':')  as datetime) as etd,
cast(replace(eta,'.',':')  as datetime) as eta,
cast(replace(atd,'.',':')  as datetime) as atd,
cast(replace(toff,'.',':') as datetime)  as toff,
cast(replace(tdwn,'.',':') as datetime)  as tdwn,
cast(replace(ata,'.',':')  as datetime) as ata,
depdelay1,
depdur1,
depdelay2,
depdur2,
depdelay3,
depdur3,
gate,
orig_actyp,
actyp,
right(ac,3) as ac,
arrdelay1,
arrdur1,
arrdelay2,
arrdur2,
arrdelay3,
arrdur3,
depgate,
arrgate,
row_number() over (partition by datop,fltid,depstn,arrstn,status order by datop,fltid,depstn,arrstn,status) as duplicado,
legno
from
(
	select 
		* 
	from 
	ODS_TEMPORALES.dbo.ODS_OPE_I_LEG
	UNION all
	select 
		*,
		'' as CHARTERER 
	from ODS_TEMPORALES.dbo.ODS_OPE_I_PLAN_LEG
) a
left join ODS_AUXILIAR.dbo.ZONA_HORARIA b on
	a.DEPSTN=b.iata
order by datop;</sql>
