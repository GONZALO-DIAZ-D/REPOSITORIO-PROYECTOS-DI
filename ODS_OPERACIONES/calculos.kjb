from ODS_OPERACIONES.dbo.O_ODS_OPERACIONES a
left join ODS_OPERACIONES.dbo.O_PARAMETRO_ACARS b on
		a.ac=b.matricula
	and a.operation_date=b.fecha
	and a.boardpoint=b.origen
	and a.offpoint=b.destino
where b.parametro is not null
and parametro='ENG1_STOP_REP';

update a
set 
engine_2_start=cast(CONVERT(DATETIME, CONVERT(varchar(20), b.fecha,101)  + ' ' + CONVERT(varchar(8), b.hora, 108)) as datetime)
from ODS_OPERACIONES.dbo.O_ODS_OPERACIONES a
left join ODS_OPERACIONES.dbo.O_PARAMETRO_ACARS b on
		a.ac=b.matricula
	and a.operation_date=b.fecha
	and a.boardpoint=b.origen
	and a.offpoint=b.destino
where b.parametro is not null
and parametro='ENGINE2_START';

update a
