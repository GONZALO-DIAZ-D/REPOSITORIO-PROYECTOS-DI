  <step>
    <name>SEGMENT_BOOK_BIR</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>52.43.16.101 - BIR_RXX_SKY</connection>
    <sql>select
	   z.id_flight  
	  ,cast(d.date as date) as view_date
	  ,z.flightnumber as flight_number
      ,a.departurecitycode as departure_city_code
      ,a.arrivalcitycode       as arrival_city_code
      ,cast(a.departuredate as date) as departuredate
      ,a.departuretime as departure_time
      ,cast(a.arrivaldate as date) as arrival_date
      ,a.arrivaltime as arrival_time	
	  ,c.type_code
	  ,b.class as fare_class
	  ,case when b.class in ('E','O') then 'NO PAGO' else 'PAGO' end as pax_type
	  ,c.fare_basis_code
	  ,case when e.tipo2 is null and fare_basis_code like '%LIGHT%' then 'LIGHT'
	   		when e.tipo2 is null and fare_basis_code like '%PLUS%'  then 'PLUS'
			else e.tipo2
	   end as fare_type
	  ,case when c.lift_status=0 then 'DEFAULT'
			when c.lift_status=1 then 'CHECKED IN'
			when c.lift_status=2 then 'BOARDED'
			when c.lift_status=3 then 'NO SHOW'
		end as lift_status
	  ,isnull(sum(c.booked),0) as booked
  FROM 	bir_rxx_sky.dbo.flight_bir z

  left join bir_rxx_sky.dbo.segment_bir a on  
		a.id_flight=z.id_flight

  left join bir_rxx_sky.dbo.segment_class_bir b on
		a.id_segment=b.id_segment

  left join bir_rxx_sky.dbo.det_segment_class_booking_bir c on
		b.id_segment_class=c.id_segment_class

  left join bir_rxx_sky.dbo.collection_date d on
		d.id_collection=z.id_collection

  left join base_apoyo.dbo.lk_farebasis e on
		c.fare_basis_code  collate database_default = e.farebasis  collate database_default
 where
		cast(d.date as date)='${fecha_proceso}'
	and cast(a.departuredate as date)>dateadd(dd,-2,cast(d.date as date))
 group by
	 z.id_flight
	,d.date
	,z.flightnumber
    ,a.departurecitycode
    ,a.arrivalcitycode
    ,a.departuredate
    ,a.departuretime
    ,a.arrivaldate
    ,a.arrivaltime
	,c.type_code
	,b.class
	,c.fare_basis_code
	,e.tipo2
	,c.lift_status
having isnull(sum(c.booked),0)>0</sql>
    <limit>0</limit>
    <lookup/>
