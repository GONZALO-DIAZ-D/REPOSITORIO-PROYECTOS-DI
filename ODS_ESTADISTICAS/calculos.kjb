    <entry>
      <name>corrige mono tramos</name>
      <description/>
      <type>SQL</type>
      <sql>
update a -- MAPEA VUELOS OPERACIONALES POR FECHA, NUMERO VUELO, ORIGEN O DESTINO
set fecha_vuelo_comercial=b.departure_date,
	vuelo_comercial=b.flight_number,
	origen_comercial=b.boardpoint,
	destino_comercial=b.offpoint,
	pax_light=b.pax_light,
    pax_plus=b.pax_plus,
    pax_no_pago=b.pax_no_pago,
    pax_pago=b.pax_light+b.pax_plus,
    pax_total=b.pax_total,
    capacity=b.capacity,
    travel_distance=b.travel_distance,
    ask=b.ask,
    rpk_light=b.rpk_light,
    rpk_plus=b.rpk_plus,
    rpk_no_pago=b.rpk_no_pago,
    rpk_pago=b.rpk_light+b.rpk_plus,
    rpk_total=b.rpk_total,
    agrupacion=c.agrupacion,
    routing=b.routing,
    routing_eerr=c.routing_eerr,
    ruta=c.ruta,
    subruta=c.subruta
from ODS_ESTADISTICAS.DBO.I_MAPEO_CONTINGENCIA a
inner join ODS_TEMPORALES.[dbo].[ODS_EST_T_RESUMEN_PAX] b on
		a.local_date=b.departure_date
	and (a.flight_number=b.flight_number or a.flight_number=(b.flight_number+1000))
	and (a.boardpoint=b.boardpoint or a.offpoint=b.offpoint)
left join ODS_AUXILIAR.dbo.MODELO_RUTA c on
		b.routing=c.routing
where a.local_date between '${fecha_inicio}' and '${fecha_fin}';

-- CORRIGE DISTANCIAS DE VUELOS CONTINGENCIAS

update a
	set travel_distance=distancia,
		ask=distancia*capacity,
		rpk_light=pax_light*distancia,
		rpk_no_pago=pax_no_pago*distancia,
		rpk_pago=pax_pago*distancia,
		rpk_plus=pax_plus*distancia,
		rpk_total=pax_total*distancia
from ODS_ESTADISTICAS.dbo.I_MAPEO_CONTINGENCIA a
left join ODS_AUXILIAR.dbo.DISTANCIA b on
	(case when boardpoint>offpoint then offpoint+boardpoint else boardpoint+offpoint end)=b.tramo_nd
where distancia&lt;&gt;travel_distance
	and local_date between '${fecha_inicio}' and '${fecha_fin}';</sql>
