      <name>mapeo_contingencia</name>
      <description/>
      <type>SQL</type>
      <sql>INSERT INTO ODS_ESTADISTICAS.DBO.I_MAPEO_CONTINGENCIA 
SELECT 
	b.operation_date,
	b.local_date,
	b.flight_number,
	b.boardpoint,
	b.offpoint,	
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null,
	null
  FROM ODS_TEMPORALES.dbo.ODS_EST_T_RESUMEN_PAX A
  RIGHT JOIN ODS_OPERACIONES.DBO.O_ODS_OPERACIONES B ON 
		A.FLIGHT_NUMBER=B.FLIGHT_NUMBER
	AND A.BOARDPOINT=B.BOARDPOINT
	AND A.OFFPOINT=B.OFFPOINT
	AND A.DEPARTURE_DATE=B.LOCAL_DATE
   left JOIN ODS_ESTADISTICAS.dbo.I_MAPEO_CONTINGENCIA C ON -- SE CRUZA CONTRA EL MAPEO PARA NO VOLVER A INSERTAR UN VUELO MAPEADO
		b.FLIGHT_NUMBER=C.flight_number
	AND b.local_date=C.local_Date
	AND b.BOARDPOINT=C.boardpoint
	AND b.OFFPOINT=C.offpoint  
WHERE 
		B.STATUS NOT IN ('CNL','RTR') 
	AND A.FLIGHT_NUMBER IS NULL
	AND B.local_date BETWEEN '${fecha_inicio}' AND '${fecha_fin}'
	AND B.flight_number&lt;5000
	AND B.boardpoint&lt;&gt;B.offpoint
	AND C.flight_number is null  -- VUELOS QUE FALTAN MAPEAR, LA IDEA ES SETEAR VARIABLE PARA DETENER EL PROCESO HASTA MAPEAR TODO</sql>
