CASE 
 WHEN ((CAST(A.U_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.U_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS U_SA,
CASE
 WHEN CAST((REPLACE(A.Lowest_Fare_H2,'.00','')) AS INTEGER) = 0 THEN 0
 ELSE CAST((REPLACE(A.Lowest_Fare_H2,'.00','')) AS INTEGER) - 7487 
END AS Curr_H2_Fare,
CASE
 WHEN CAST((REPLACE(A.Lowest_Fare_LA,'.00','')) AS INTEGER) = 0 THEN 0
 ELSE CAST((REPLACE(A.Lowest_Fare_LA,'.00','')) AS INTEGER) - 7487 
END AS Grid_Fare_LA,
CASE 
 WHEN CAST((REPLACE(A.Lowest_Fare_JA,'.00','')) AS INTEGER) = 0 THEN 0
 ELSE CAST((REPLACE(A.Lowest_Fare_JA,'.00','')) AS INTEGER) - 7487 
END AS JA,
CASE
 WHEN CAST((REPLACE(A.Lowest_Fare_JA,'.00','')) AS INTEGER) = 0 THEN 0
 ELSE CAST((REPLACE(A.Lowest_Fare_JA,'.00','')) AS INTEGER) - 7487 
END AS Lowest_JA,
SUBSTRING(A.Flightline,1,6) AS OD,
SUBSTRING(A.Flightline,8,3) AS Numero_vuelo,
DAY(CAST(A.Fecha AS DATE)) AS Dia,
MONTH((CAST(A.Fecha AS DATE))) AS Mes,
YEAR((CAST(A.Fecha AS DATE))) AS Año,
DATEPART(DW, (CAST(A.Fecha AS DATE))) AS DOW,
CASE
 WHEN RIGHT(A.Rule_Name,7)='Feriado' THEN 'Feriado'
 ELSE RIGHT(A.Rule_Name,1)
END AS Cluster,
CASE
 WHEN LEN(A.FlightPath)=6 THEN 0
 ELSE 1
END AS Stops,
B.subruta AS Subruta,
CASE 
 WHEN (SUBSTRING(A.Flightline,8,3) = 1 OR SUBSTRING(A.Flightline,8,3) = 2 OR SUBSTRING(A.Flightline,8,3) = 11 
 OR SUBSTRING(A.Flightline,8,3) = 12 OR SUBSTRING(A.Flightline,8,3) = 13 OR SUBSTRING(A.Flightline,8,3) = 14) THEN CAST(A.Tot_Cap AS INTEGER)/2
 ELSE (CAST(A.Tot_Cap AS INTEGER))
END AS Cap_Aux_Y,
DATEDIFF(dd, CAST(@fecha_vista AS DATE), CAST(A.Fecha AS DATE)) AS AP,
CASE
 WHEN DATEDIFF(dd, CAST(@fecha_vista AS DATE), CAST(A.Fecha AS DATE)) &lt; 0 THEN 0
 ELSE 1
END AS Contador,
(CAST((REPLACE(A.Lowest_Fare_H2,'.00','')) AS INTEGER) - CAST((REPLACE(A.Lowest_Fare_LA,'.00','')) AS INTEGER)) AS Dif_vs_LA,
(CAST((REPLACE(A.Lowest_Fare_H2,'.00','')) AS INTEGER) - CAST((REPLACE(A.Lowest_Fare_JA,'.00','')) AS INTEGER)) AS Dif_vs_JA,
CASE
 WHEN (CAST((REPLACE(A.Lowest_Fare_H2,'.00','')) AS INTEGER) - CAST((REPLACE(A.Lowest_Fare_LA,'.00','')) AS INTEGER)) &lt;= 0 THEN 1
 ELSE 0 
END AS Competitivo,
CASE
 WHEN (CAST((REPLACE(A.Lowest_Fare_H2,'.00','')) AS INTEGER) - CAST((REPLACE(A.Lowest_Fare_LA,'.00','')) AS INTEGER)) > 0 THEN 1
 ELSE 0 
END AS Descompetitivo,
CASE
 WHEN (CAST((REPLACE(A.Lowest_Fare_H2,'.00','')) AS INTEGER) - CAST((REPLACE(A.Lowest_Fare_LA,'.00','')) AS INTEGER)) = 0 THEN 1
 ELSE 0 
END AS Neutro,
CASE
 WHEN ((CAST(A.X_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) > 0 THEN 1
 ELSE 0
END AS Dispo_X,
CASE
 WHEN ((CAST(A.A_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) > 0 THEN 1
 ELSE 0
END AS Dispo_A,
CASE
 WHEN ((CAST(A.U_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) > 0 THEN 1
 ELSE 0
END AS Dispo_U,
CASE
 WHEN LEFT(A.FlightPath,3)&lt;RIGHT(A.FlightPath,3) THEN CONCAT(LEFT(A.FlightPath,3),RIGHT(A.FlightPath,3))
 ELSE CONCAT(RIGHT(A.FlightPath,3),LEFT(A.FlightPath,3))
END AS OD_NO_DIR,
B.agrupacion AS AGRUPACION,
CASE 
 WHEN (SUBSTRING(A.Flightline,8,3)/2) = 0 THEN 'PAR'
 ELSE 'IMPAR'
END AS PAR_O_IMPAR,
CASE
 WHEN CAST(A.G_Bk AS INTEGER) = 0 THEN 'SIN'
 WHEN CAST(A.G_Bk AS INTEGER) &lt; 31 THEN 'BAJO'
 WHEN CAST(A.G_Bk AS INTEGER) &lt; 51 THEN 'MEDIO'
 WHEN CAST(A.G_Bk AS INTEGER) &lt; 76 THEN 'ALTO'
 ELSE 'MUY ALTO'
END AS Grupos,
CASE 
 WHEN DATEDIFF(dd, CAST(@fecha_vista AS DATE), CAST(A.Fecha AS DATE)) &lt; 0 THEN '-1'
 WHEN DATEDIFF(dd, CAST(@fecha_vista AS DATE), CAST(A.Fecha AS DATE)) BETWEEN 0 AND 7  THEN '0-7'
 WHEN DATEDIFF(dd, CAST(@fecha_vista AS DATE), CAST(A.Fecha AS DATE)) BETWEEN 8 AND 14 THEN '8-14'
 WHEN DATEDIFF(dd, CAST(@fecha_vista AS DATE), CAST(A.Fecha AS DATE)) BETWEEN 15 AND 21 THEN '15-21'
 WHEN DATEDIFF(dd, CAST(@fecha_vista AS DATE), CAST(A.Fecha AS DATE)) BETWEEN 22 AND 29 THEN '22-29'
 WHEN DATEDIFF(dd, CAST(@fecha_vista AS DATE), CAST(A.Fecha AS DATE)) BETWEEN 30 AND 59 THEN '30-59'
 WHEN DATEDIFF(dd, CAST(@fecha_vista AS DATE), CAST(A.Fecha AS DATE)) BETWEEN 60 AND 89 THEN '60-89' 
 ELSE '90+' 
END AS RANGO_AP
FROM RMS_tmp A
LEFT JOIN vuelo_routing_final B ON CAST(A.Fecha AS DATE) = B.fecha_vuelo
and SUBSTRING(A.Flightline,8,3) = B.numero_vuelo</sql>
