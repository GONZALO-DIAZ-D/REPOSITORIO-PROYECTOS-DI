CASE 
 WHEN ((CAST(A.A_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.A_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
