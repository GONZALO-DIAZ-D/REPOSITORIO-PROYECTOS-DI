    <connection>10.30.1.168_Temporales</connection>
    <sql>
DECLARE @fecha_vista DATE;
--SET @fecha_vista = '2018-01-03';
SET @fecha_vista = GETDATE();

SET DATEFIRST 1;

SELECT 
CAST(@fecha_vista AS DATE) AS fecha_vista,
A.Flightline,
A.FlightPath,
CAST(A.Fecha AS DATE) AS Fecha,
CAST(A.Wk AS INTEGER) AS Wk, 
A.Dept_Time,
CAST(A.Tot_Cap AS INTEGER) AS Total_Cap,
CAST(A.Tot_Lid AS INTEGER) AS Total_Lid,
CAST(A.Tot_Bkd AS INTEGER) AS Total_Bkgs,
CAST(A.Total_Revenue AS INTEGER) AS Total_Revenue,
CAST(A.Tot_Pickup_1 AS INTEGER) AS Pickup_1,
CAST(A.Tot_Pickup_3 AS INTEGER) AS Pickup_3,
CAST(A.Tot_Pickup_5 AS INTEGER) AS Pickup_5,
CAST(A.Tot_Pickup_7 AS INTEGER) AS Pickup_7,
CAST(A.Tot_Pickup_14 AS INTEGER) AS Pickup_14,
CAST(A.Tot_Pickup_21 AS INTEGER) AS Pickup_21,
CAST(A.TotRevPkup_1 AS INTEGER) AS Rev_P1,
CAST(A.TotRevPkup_3 AS INTEGER) AS Rev_P3,
CAST(A.TotRevPkup_5 AS INTEGER) AS Rev_P5,
CAST(A.TotRevPkup_7 AS INTEGER) AS Rev_P7,
CAST(A.TotRevPkup_14 AS INTEGER) AS Rev_P14,
CAST(A.TotRevPkup_21 AS INTEGER) AS Rev_P21,
A.Rule_Name,
CONVERT(FLOAT,A.TotCap_LF) AS Total_LF_Cap,
CONVERT(FLOAT,A.TotLid_LF) AS Total_LF_Lid,
CAST((REPLACE(A.Lowest_Fare_LA,'.00','')) AS INTEGER) AS Fare_LA_con_tasas,
CAST(A.X_Bk AS INTEGER) AS Bkg_X,
CAST(A.A_Bk AS INTEGER) AS Bkg_A,
CAST(A.U_Bk AS INTEGER) AS Bkg_U,
CAST(A.G_Bk AS INTEGER) AS Bkg_G,
CAST(A.G_Rev AS INTEGER) AS Rev_G, 
CAST(A.G_PKUp_1_Bk AS INTEGER) AS Pickup_1_G,
CASE
  WHEN CAST((REPLACE(A.Lowest_Fare_LA,'.00','')) AS INTEGER) = 0 THEN '-' 
  ELSE CAST((REPLACE(A.Lowest_Fare_LA,'.00','')) AS INTEGER) 
END AS Lowest_Fare_LA,
CASE 
 WHEN ((CAST(A.Y_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.Y_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS Y_SA,
CASE 
 WHEN ((CAST(A.K_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.K_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS K_SA,
CASE 
 WHEN ((CAST(A.V_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.V_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS V_SA,
CASE 
 WHEN ((CAST(A.H_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.H_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS H_SA,
CASE 
 WHEN ((CAST(A.S_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.S_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS S_SA,
CASE 
 WHEN ((CAST(A.B_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.B_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS B_SA,
CASE 
 WHEN ((CAST(A.L_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.L_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS L_SA,
CASE 
 WHEN ((CAST(A.R_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.R_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS R_SA,
CASE 
 WHEN ((CAST(A.T_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.T_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS T_SA,
CASE 
 WHEN ((CAST(A.M_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.M_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS M_SA,
CASE 
 WHEN ((CAST(A.N_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.N_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS N_SA,
CASE 
 WHEN ((CAST(A.Q_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.Q_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS Q_SA,
CASE 
 WHEN ((CAST(A.Z_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(A.Z_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
END AS Z_SA,
CASE 
 WHEN ((CAST(X_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER))) &lt; 0 THEN 0
 ELSE ((CAST(X_Au AS INTEGER))- (CAST(A.Tot_Bkd AS INTEGER)))  
