    <connection>AZURE_DM_CONTACT_CENTER</connection>
    <sql>DECLARE @fecha_proceso DATE;
SET @fecha_proceso = GETDATE();
--SET @fecha_proceso = '${fecha_proceso}';

SELECT 
A.id_llamada,
A.id_grabacion,
A.grupo_ACD,
A.estado,
A.calificacion,
A.clasificacion,
A.sub_clasificacion,
CASE
 WHEN A.transferida IS NOT NULL THEN 'SI'
 ELSE 'NO'
END AS transferida ,
A.destino,
A.ani
FROM (SELECT ROW_NUMBER() OVER(PARTITION BY B.id_llamada ORDER BY id_llamada DESC) AS CLAVE, B.* FROM DETALLE_LLAMADAS B) A
WHERE CAST(A.fecha_llamada AS DATE) = DATEADD(dd,${DIAS},(CAST(@fecha_proceso AS DATE)))
AND A.CLAVE = 1
GROUP BY 
A.id_llamada,
A.id_grabacion,
A.grupo_ACD,
A.estado,
A.calificacion,
A.clasificacion,
A.sub_clasificacion,
A.transferida,
A.destino,
A.ani</sql>
