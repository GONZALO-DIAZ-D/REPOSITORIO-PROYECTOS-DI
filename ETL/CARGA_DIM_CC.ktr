    <sql>DECLARE @fecha_proceso DATE;
SET @fecha_proceso = GETDATE();
--SET @fecha_proceso = '${fecha_proceso}';

SELECT
nombre, 
apellido_paterno, 
apellido_materno, 
usuario 
FROM DETALLE_LLAMADAS
WHERE CAST(fecha_llamada AS DATE) = DATEADD(dd,${DIAS},(CAST(@fecha_proceso AS DATE)))
AND usuario NOT IN (SELECT DISTINCT(usuario) FROM USUARIOS)
GROUP BY nombre, 
apellido_paterno, 
apellido_materno, 
usuario </sql>
